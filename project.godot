config/icon="res://Assets/tamu.png"
config/windows_native_icon="res://Assets/icon.ico"

[autoload]

Globals="*res://Globals/Globals.gd"

[display]

window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godotoro/plugin.cfg" )

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
